                    SelectedItem="{Binding SelectCombo.Value}" />
                <Button
                    Grid.Row="1"
                    Width="80"
                    Height="30"
                    Margin="30,25,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Controls:ControlsHelper.ContentCharacterCasing="Normal"
                    Command="{Binding PlotRun}"
                    Content="Plot"
                    FontFamily="Segoe UI, Lucida Sans Unicode, Verdana"
                    FontSize="14"
                    FontWeight="Normal" />
                <CheckBox
                    Grid.Column="1"
                    Height="18"
                    Margin="0,18,180,0"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    Content="Log(Vertical)"
                    IsChecked="{Binding AxisY.Value}" />
                <CheckBox
                    Grid.Column="1"
                    Height="18"
                    Margin="0,18,40,0"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    Content="Log(Horizontal)"
                    IsChecked="{Binding AxisX.Value}" />
            </Grid>
        </TabItem>

    </TabControl>
</Controls:MetroWindow>
